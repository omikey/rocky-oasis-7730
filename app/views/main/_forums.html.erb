<table id="forums">
  <tr>
    <td style="height:2em">
      <h1>
        Community
      </h1>
    </td>
  </tr>
  <tr style="font-size: 1.1em; font-weight: 600">
    <td style="font-size: 1.1em; width:50%">
      Forums
    </td>
    <td style="font-size: 1.1em; width:15%">
      Queries
    </td>
    <td style="font-size: 1.1em; width:15%">
      Responses
    </td>
    <td style="font-size: 1.1em; width:15%">
      Updated
    </td>
  </tr>
  <tr ng-repeat="forum in data">
    <td class="link"
        ng-click="forumulate(forum.id)"
        style="font-size: 1.1em;">
      {{forum.title}}
    </td>
    <td style="font-size: 1.1em;">
      {{forum.queries}}
    </td>
    <td style="font-size: 1.1em;">
      {{forum.responses}}
    </td>
    <td style="font-size: 1.1em;">
      {{forum.updated}}
    </td>
  </tr>
  <tr>
  </tr>
</table>